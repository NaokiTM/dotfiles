    "display": {
        "separator": "  "
    },
    "modules": [
        "break",
        {
            "type": "title",
	    "color" :{
		  "user":"#86B5F3",
		  "at": "#ABCBF6",
		  "host": "#A7CAF9"
	    },
            "key": "┌  ",
