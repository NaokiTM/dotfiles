        {
            "type": "kernel",
            "keyColor": "#d3dff0",
            "key": "├  "
        },
        {
            "type": "wm",
            "key": "├  ",
            "keyColor": "#bacae2"
        },
        {
            "type": "mouse",
            "key": "├ 󰍽 ",
            "keyColor": "#a2b6d5"
        },
        {
            "type": "cpu",
            "key": "├  ",
            "keyColor": "#8aa2c8"
        },
        {
            "type": "gpu",
            "key": "├  ",
            "keyColor": "#748ebb"
        },
        {
            "type": "display",
            "key": "├ 󰹑 ",
            "keyColor": "#5d7aae"
